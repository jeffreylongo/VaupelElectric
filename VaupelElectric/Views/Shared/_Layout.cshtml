
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1"> 
    <link rel="stylesheet" type="text/css" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css">
    <title>@ViewBag.Title - Vaupel Electric</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <link rel="apple-touch-icon" sizes="180x180" href="/apple-touch-icon.png">
    <link rel="icon" type="image/png" sizes="32x32" href="/favicon-32x32.png">
    <link rel="icon" type="image/png" sizes="16x16" href="/favicon-16x16.png">
    <link rel="manifest" href="/manifest.json">
    <meta name="theme-color" content="#ffffff">
</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a href="/">
                    <img src="/Images/VE-notag.png"  alt="Vaupel Electric" height="50"/>
                </a>

            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li class="home">@Html.ActionLink("Home", "Index", "Home")</li>
                    <li class="team">@Html.ActionLink("Our Team", "Team", "Home")</li>
                    <li class="services">@Html.ActionLink("Services", "Services", "Home")</li>
                    <li class="prices">@Html.ActionLink("Prices", "Prices", "Home")</li>
                    <li class="testis">@Html.ActionLink("Testimonials", "Testimonials", "Home")</li>
                    <li class="contact">@Html.ActionLink("Contact", "Contact", "Home")</li>
                </ul>
                @*<strong class="bigNumber">(941) 284-7120</strong>*@
                @*<div class="scheduleButtonL">
                    <a href="tel:9412847120" class="btn btn-primary btn-sm"><span class="glyphicon glyphicon-phone-alt"></span> Call: (941) 284-7120</a>
                </div>*@
            </div>
        </div>
    </div>
    <div class="container body-content">
        @RenderBody()
        <footer>
            <p>&copy; @DateTime.Now.Year - Vaupel Electric, LLC</p>
        </footer>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
